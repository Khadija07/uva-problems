#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t, a, f;
    cin>>t;
    for(int i=0; i<t; i++)
    {
        cin>>a>>f;
        if(i>0)
        {
            printf("\n");
        }
        for(int l=0; l<f; l++)
        {
            if(l>0 && l!=f)
            {
                printf("\n");

            }
            for(int j=1; j<=a; j++)
            {
                for(int k=0; k<j; k++)
                {
                    cout<<j;
                }
                printf("\n");
            }
            int b=a;
            for(int j=0; j<a-1; j++)
            {
                b = a - j;
                for(int k=j; k<a-1; k++)
                {
                    cout<<b-1;
                }
                printf("\n");
            }


    }
}

}
